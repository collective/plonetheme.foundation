<ul data-orbit tal:attributes="data-options view/options" tal:condition="view/slides">
  <li tal:repeat="slide view/slides">
    <img tal:replace="structure slide/picturefill"/>
    <div class="orbit-caption" tal:replace="nothing">
           <span tal:replace="slide/title">Title</span>
    </div>
  </li>
</ul>